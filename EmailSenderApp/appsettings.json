{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AzureCommunicationService": {
    "ConnectionString": "<your_connection_string>",
    "SenderAddress": "<your_verified_sender_address>"
  },
  "OAuthSettings": {
    "ClientId": "your_client_id",
    "ClientSecret": "your_client_secret",
    "RedirectUri": "https://ourapp/api/auth/callback",
    "AuthorizationEndpoint": "https://fhir.nextgen.com/nge/prod/patient-oauth/authorize",
    "TokenEndpoint": "https://fhir.nextgen.com/nge/prod/patient-oauth/token",
    "ResponseType": "code",
    "Scope": "patient/*.read",
    "State": "some_random_state_value"
  },

  "AllowedHosts": "*"
}
